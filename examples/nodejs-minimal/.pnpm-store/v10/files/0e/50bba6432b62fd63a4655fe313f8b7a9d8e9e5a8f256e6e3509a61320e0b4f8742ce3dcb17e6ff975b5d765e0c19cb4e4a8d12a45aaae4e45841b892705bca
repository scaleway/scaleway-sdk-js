  name: {
    maxLength: 50,
    minLength: 3,
    pattern: /^[A-Za-z0-9-_. ]+$/
  },
  retentionDays: {
    greaterThanOrEqual: 1,
    ignoreEmpty: true,
    lessThanOrEqual: 1825
  }
};
