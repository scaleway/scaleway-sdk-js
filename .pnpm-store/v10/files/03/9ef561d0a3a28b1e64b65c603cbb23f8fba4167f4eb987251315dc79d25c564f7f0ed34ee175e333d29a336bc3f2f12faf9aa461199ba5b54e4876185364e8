import 'vite-node/constants';

class ThreadsVmWorker {
	getRpcOptions(ctx) {
		return createThreadsRpcOptions(ctx);
	}
	runTests(state) {
		return runVmTests("run", state);
	}
	collectTests(state) {
		return runVmTests("collect", state);
	}
}
const worker = new ThreadsVmWorker();

export { worker as default };
